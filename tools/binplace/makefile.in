# ============================================================================
# File: makefile.in
# makefile for binplace
# ============================================================================

# Common part
TOP_SRC_DIR = @top_srcdir@
include $(TOP_SRC_DIR)/makefile.common

# Local changes
TARGET_NAME = $(TARGETCOMPLUS)/binplace

# Source files
SOURCES = binplace.c

# Objects
OBJS = $(SOURCES:.c=.o)

# Targets

all: $(TARGET_NAME)

$(TARGET_NAME): $(OBJS)
	ld -o $@ $(LDFLAGS) $(LINKER_FLAGS) $(REQUIRED_CRUNTIME) -l$(PAL_LIB) $(OBJS) $(GCC_LIB)

clean:
	rm -f *.o
	rm -f $(TARGET_NAME)
